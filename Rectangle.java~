public class Rectangle extends Shape
{
   public Rectangle( int width, int height ) {
      super( width, height );
   }
   public double getArea(){
      return this.getSide1() * this.getSide2();
   }
   public double getPerimeter(){
      return 2*(this.getSide1() + this.getSide2());
   }
}